// -----------------------------------------------------------------------------

bool BtagSystematicPlots::hasTrueZMuMu( const Event::Data& ev ){

  for( std::vector<Event::GenObject>::const_iterator igen = ev.GenParticles().begin(); igen != ev.GenParticles().end(); ++igen ) {
    if( isTrueZMuMu( (*igen) ) ){
      return true;
    }
  }
  return false;
}


// -----------------------------------------------------------------------------
//
bool BtagSystematicPlots::isTrueZMuMu( const Event::GenObject& gob ){
  bool islep=false;
  if(gob.GetStatus() == 3 ) {
    if(fabs( gob.GetID() ) == 13 ) {
      if(fabs( gob.GetMotherID() ) == 23) {
  islep=true;
      }
    }
  }
  return islep;
}



